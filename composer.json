{
    "name": "sevens/vars",
    "description": "Simple Variable library Package For Encoding and Manipulating Strings and Arrays.",
    "type": "library",
    "version": "v2.1.0",
    "license": "MIT",
    "minimum-stability": "dev",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Seven\\Vars\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2.0"
    },
    "authors": [
        {
            "name": "Elisha Temiloluwa a.k.a TemmyScope",
            "email": "temmyscope@protonmail.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "encode", "encrypt", "decrypt",
        "array manipulator", "string"
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.4.2"
    }
}
