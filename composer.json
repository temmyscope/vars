{
    "name": "sevens/vars",
    "description": "Simple Variable library Package For Encoding and Manipulating Strings and Arrays.",
    "type": "library",
    "version": "v1.5.0",
    "license": "MIT",
    "minimum-stability": "dev",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
