            "Seven\\Vars\\Strings": "Strings.php",
            "Seven\\Vars\\Arrays": "Arrays.php",
            "Seven\\Vars\\Validation": "Validation.php"
        }
    },
    "require": {
        "php": ">=7.2.0"
    },
    "authors": [
        {
            "name": "Elisha Temiloluwa a.k.a TemmyScope",
            "email": "temmyscope@protonmail.com",
            "role": "Developer"
        }
